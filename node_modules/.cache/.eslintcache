\n      await fetch(`/feedback/${id}`, {\n        method: 'DELETE'\n      })\n\n      setFeedback(feedback.filter((item) => item.id !== id))\n    }\n  }\n\n  // Update feedback item\n  const updateFeedback = async (id, updItem) => {\n    const response = await fetch(`/feedback/${id}`, {\n      method: 'PUT',\n      headers: {\n        'Content-Type': 'application/json',\n      },\n      body: JSON.stringify(updItem)\n    })\n\n    const data = response.json()\n\n    setFeedback(feedback.map((item) => (item.id === id ? data : item)))\n\n    setFeedbackEdit({\n      item: {},\n      edit: false,\n    })\n  }\n\n  // Set item to be updated\n  const editFeedback = (item) => {\n    setFeedbackEdit({\n      item,\n      edit: true\n    })\n  }\n\n  return <FeedbackContext.Provider value={{\n    feedback,\n    feedbackEdit,\n    isLoading,\n    deleteFeedback,\n    addFeedback,\n    editFeedback,\n    updateFeedback,\n  }}>\n    {children}\n  </FeedbackContext.Provider>\n}\n\nexport default FeedbackContext",[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/FeedbackList.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/Header.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/FeedbackStats.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/FeedbackForm.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/AboutIconLink.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/pages/AboutPage.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/FeedbackItem.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/RatingSelect.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/shared/Spinner.jsx",[],[],"/Users/nick/Desktop/vscode/Feedback-App/Feedback-Application/src/components/share